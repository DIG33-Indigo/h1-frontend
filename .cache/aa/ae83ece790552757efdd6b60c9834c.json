olumn":2761}},{"source":"views/pages/userDashboard.js","name":"App","original":{"line":75,"column":21},"generated":{"line":1,"column":2763}},{"source":"views/pages/userDashboard.js","name":"rootEl","original":{"line":75,"column":25},"generated":{"line":1,"column":2765}},{"source":"views/pages/userDashboard.js","name":"rootEl","original":{"line":75,"column":25},"generated":{"line":1,"column":2773}},{"source":"views/pages/userDashboard.js","original":{"line":79,"column":15},"generated":{"line":1,"column":2782}},{"source":"views/pages/userDashboard.js","original":{"line":79,"column":15},"generated":{"line":1,"column":2786}},{"source":"views/pages/userDashboard.js","original":{"line":79,"column":15},"generated":{"line":1,"column":2788}},{"source":"views/pages/userDashboard.js","name":"UserDashboardView","original":{"line":79,"column":19},"generated":{"line":1,"column":2792}},{"source":"views/pages/userDashboard.js","original":{"line":79,"column":15},"generated":{"line":1,"column":2794}},{"source":"views/pages/userDashboard.js","original":{"line":79,"column":15},"generated":{"line":1,"column":2802}},{"source":"views/pages/userDashboard.js","original":{"line":79,"column":15},"generated":{"line":1,"column":2810}}],"sources":{"views/pages/userDashboard.js":"import App from '../../App'\nimport {html, render } from 'lit-html'\nimport {gotoRoute, anchorRoute } from '../../Router'\nimport Utils from '../../Utils'\nimport Auth from './../../Auth'\nimport moment from 'moment'\n\nclass UserDashboardView {\n  init(){    \n    console.log('UserDashboardView.init')\n    document.title = 'Me & My Trips'    \n    this.render()    \n    Utils.pageIntroAnim()    \n  }\n\n//profile code section --------------------------------------------\n\n\n\n  render(){\n    const template = html`\n      <style>\n        h3 {\n          text-align: centre;\n        }\n\n      </style>\n\n\n\n\n     <td-app-header title=\"My & My Trips\" user=${JSON.stringify(Auth.currentUser)}></td-app-header>\n      <div class=\"page-content\">\n      \n       \n        <h3>User Daashboard</h3>\n\n\n        <sl-tab-group>\n          <sl-tab slot=\"nav\" panel=\"next\">My Next Trip</sl-tab>\n          <sl-tab slot=\"nav\" panel=\"chat\">Tour Chat</sl-tab>\n          <sl-tab slot=\"nav\" panel=\"favourites\">Favourites</sl-tab>\n          <sl-tab slot=\"nav\" panel=\"profile\">Profile</sl-tab>\n\n          <sl-tab-panel name=\"next\">Get package by booking id here</sl-tab-panel>\n          <sl-tab-panel name=\"chat\">Chat Forum code to go here</sl-tab-panel>\n          <sl-tab-panel name=\"favourites\">Get package by favourite ids here</sl-tab-panel>\n          <sl-tab-panel name=\"profile\">\n            <div class=\"signinup-box\">\n               ${Auth.currentUser && Auth.currentUser.avatar ? html`\n                <sl-avatar style=\"--size: 200px; margin-bottom: 1em;\" image=${(Auth.currentUser && Auth.currentUser.avatar) ? `${App.apiBase}/images/${Auth.currentUser.avatar}` : ''}></sl-avatar>\n              `:html`\n                <sl-avatar style=\"--size: 200px; margin-bottom: 1em;\"></sl-avatar>\n              `}\n              <h2>${Auth.currentUser.firstName} ${Auth.currentUser.lastName}</h2>\n              <p>${Auth.currentUser.email}</p>\n              ${Auth.currentUser.bio ? html`\n                <h3>About Me</h3>\n                <p>${Auth.currentUser.bio}</p>\n              ` : html`\n                <p>No news about me.</p><br>\n              `}\n              <p>Updated: ${moment(Auth.currentUser.updatedAt).format('MMMM Do YYYY, @ h:mm a')}</p>\n              <sl-button @click=${()=> gotoRoute('/editProfile')}>Edit Profile</sl-button>\n              </div>         \n            </div>\n            \n          </sl-tab-panel>\n        </sl-tab-group>\n\n      </div>\n      \n    `\n\n    render(template, App.rootEl)\n  }\n}\n\nexport default new UserDashboardView()"},"lineCount":null}},"error":null,"hash":"e386919796662a4645e3474052b8685a","cacheData":{"env":{}